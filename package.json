{
  "name": "ui-core",
  "version": "0.1.0",
  "description": "UI component library built with React",
  "main": "lib/index.js",
  "scripts": {
    "build:wp": "webpack --config webpack.config.js --display-error-details",
    "dev": "babel src --out-dir lib --source-maps inline --copy-files",
    "watch": "babel src -watch --out-dir lib --source-maps inline --copy-files",
    "lint": "eslint --ext .js,.jsx src/**; exit 0",
    "lint:fix": "eslint --ext .js,.jsx src/**  --fix",
    "lint:watch": "esw -w --ext .js,.jsx src/**",
    "prepare": "npm run watch",
    "test": "jest --env=jsdom"
  },
  "author": "Bob Morganstern <bamorgans@yahoo.com>",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.9",
    "@fortawesome/fontawesome-free-solid": "^5.0.9",
    "@fortawesome/react-fontawesome": "0.0.18",
    "antd": "^3.4.0",
    "enumify": "^1.0.4",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "jest": "^22.4.3",
    "prop-types": "^15.6.1",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-cli": "^2.0.13"
  },
  "setupFiles": [
    "<rootDir>/enzyme-setup.js"
  ]
}
