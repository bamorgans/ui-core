{
  "name": "ui-core",
  "version": "0.1.0",
  "description": "UI component library built with React",
  "main": "lib/ui-core.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --config webpack.prod.config.js",
    "dev": "NODE_ENV=development webpack --config webpack.dev.config.js --display-error-details",
    "watch": "NODE_ENV=development webpack -w --config webpack.dev.config.js --display-error-details",
    "dev:babel": "NODE_ENV=development babel src --out-dir lib --source-maps inline --copy-files",
    "watch:babel": "NODE_ENV=development babel src -watch --out-dir lib --source-maps inline --copy-files",
    "lint": "eslint --ext .js,.jsx src/**; exit 0",
    "lint:fix": "eslint --ext .js,.jsx src/**  --fix",
    "lint:watch": "esw -w --ext .js,.jsx src/**",
    "test": "jest --env=jsdom"
  },
  "author": "Bob Morganstern <bamorgans@yahoo.com>",
  "license": "ISC",
  "dependencies": {
    "antd": "^3.4.0",
    "enumify": "^1.0.4",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "base64-image-loader": "^1.2.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.3",
    "file-loader": "^1.1.11",
    "jest": "^22.4.3",
    "postcss-loader": "^2.1.3",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack-cli": "^2.0.13",
    "webpack-merge": "^4.1.2"
  },
  "setupFiles": [
    "<rootDir>/enzyme-setup.js"
  ]
}
